class Solution:
    def missingNumber(self, nums: List[int]) -> int:
        
        # at first, sort the array
        nums.sort()

        output = []
        
        for i in range(1, nums[-1]):
            
            if i not in nums:
                
                output.append(i)
                
        return output[i]

        # https://thecleverprogrammer.com/2022/07/13/find-missing-number-using-python/
